<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.annaru.queue.mapper.JUserInfoMapper">

	<!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.annaru.queue.model.JUserInfo" >
        <result column="id" property="id" />
        <result column="user_name" property="userName" />
        <result column="head_picture" property="headPicture" />
        <result column="title_id" property="titleId" />
        <result column="introduction" property="introduction" />
        <result column="job_number" property="jobNumber" />
        <result column="create_user" property="createUser" />
        <result column="is_delete" property="isDelete" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
    </resultMap>

    <!-- 多表页面信息查询分页 -->
    <select id="selectDataPage" resultMap="BaseResultMap">

    </select>


</mapper>